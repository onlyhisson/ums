<!DOCTYPE html>
<html lang="en">
<head>
  <%- include ../_incl/inc_header.ejs %>
</head>
<body class="hold-transition skin-black sidebar-mini">
<div class="wrapper">

  <%- include ../_incl/inc_top.ejs %>
  <%- include ../_incl/inc_submenu.ejs %>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        <%=title%>
      </h1>
      <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="#"><i class="fa fa-list-ul"></i> 충전내역</a></li>
        <li class="breadcrumb-item active"><%=title%></li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-12">


                <div class="box">
                    <!--
                    <div class="box-header with-border">
                        <h3 class="box-title">Data Table With Full Features</h3>
                    </div>
                    -->
                    <!-- /.box-header -->
                    <div class="box-body">
                        <div class="table-responsive">
                            <!--
                                <table id="example1" class="table table-bordered table-striped">
                            -->
                            <table id="example1" class="table table-striped">
                            <thead>
                                <tr class="bb-3 border-purple">
                                    <th>주문번호</th>
                                    <th data-toggle="tooltip" data-original-title="엔젤마스터카드번호">엔젤카드 No.</th>
                                    <th>Epay 주문번호</th>
                                    <th>Epay 계정</th>
                                    <th>주문한 금액</th>
                                    <th>지불화폐 종류</th>
                                    <th>입금화폐 종류</th>
                                    <th>주문서 상태</th>
                                    <th>주문서 시간</th>
                                </tr>
                            </thead>
                            <tbody>
                                <%epayList.forEach(function(el){%>
                                <tr>
                                    <td><%=el.id%></td>
                                    <td><%=el.card_no%> </td>
                                    <td><%=el.order_no%></td>
                                    <td><%=sess.email%></td>
                                    <td><%=el.price%></td>
                                    <td><%=el.from_unit%></td>
                                    <td><%=el.unit%></td>
                                    <td>
                                        <span class="label <%=epayStColor[el.status]%>"><%=epayStatus[el.status]%></span>
                                    </td>
                                    <td><%=el.create_at%></td>
                                </tr>
                                <%})%>
                            </tbody>
                            <tfoot>
                                <tr class="bt-3 border-purple">
                                    <th>주문번호</th>
                                    <th data-toggle="tooltip" data-original-title="엔젤마스터카드번호">엔젤카드 No.</th>
                                    <th>Epay 주문번호</th>
                                    <th>Epay 계정</th>
                                    <th>주문한 금액</th>
                                    <th>지불화폐 종류</th>
                                    <th>입금화폐 종류</th>
                                    <th>주문서 상태</th>
                                    <th>주문서 시간</th>
                                </tr>
                            </tfoot>
                            </table>
                        </div>
                    </div>
                    <!-- /.box-body -->
                </div>
                <!-- /.box -->  

            </div>
        </div>








     
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
  
   
  <%- include ../_incl/inc_footer.ejs %>
  <%- include ../_incl/inc_sidebar.ejs %>
  
</div>
<!-- ./wrapper -->

<%- include ../_incl/inc_scripts.ejs %>

<!-- This is data table -->
<script src="/assets2/vendor_plugins/DataTables-1.10.15/media/js/jquery.dataTables.min.js"></script>
    
<!-- start - This is for export functionality only -->
<script src="/assets2/vendor_plugins/DataTables-1.10.15/extensions/Buttons/js/dataTables.buttons.min.js"></script>
<script src="/assets2/vendor_plugins/DataTables-1.10.15/extensions/Buttons/js/buttons.flash.min.js"></script>
<script src="/assets2/vendor_plugins/DataTables-1.10.15/ex-js/jszip.min.js"></script>
<script src="/assets2/vendor_plugins/DataTables-1.10.15/ex-js/pdfmake.min.js"></script>
<script src="/assets2/vendor_plugins/DataTables-1.10.15/ex-js/vfs_fonts.js"></script>
<script src="/assets2/vendor_plugins/DataTables-1.10.15/extensions/Buttons/js/buttons.html5.min.js"></script>
<script src="/assets2/vendor_plugins/DataTables-1.10.15/extensions/Buttons/js/buttons.print.min.js"></script>
<!-- end - This is for export functionality only -->

<!-- Crypto_Admin for Data Table -->
<script src="/assets2/js/pages/data-table.js"></script>

</body>
</html>
