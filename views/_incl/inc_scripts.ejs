        kycStatus: ['제출되지 않음', '제출 후 심사중', '심사 통과', '심사 통과 되지 않음, 다시 제출 요망']
    }

    toastAlert = (title, msg, tstType) => {
        $.toast({
            heading: title,
            text: msg,
            position: 'top-right',
            loaderBg: '#ff6849',
            icon: tstType,      // info, warning, success, error
            hideAfter: 3000,
            stack: 6
        });
    }

</script>