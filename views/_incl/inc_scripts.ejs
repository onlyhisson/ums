<script>

    function replaceAll(str, searchStr, replaceStr) {
        return str.split(searchStr).join(replaceStr);
    }

    $(document).ready(function() {
        // Animate loader off screen
        $(".se-pre-con").fadeOut("slow");

        // 서브메뉴 클릭시 페이지로더 바로 띄우기
        $(".sidebar-menu").find('li a').bind('click', function(e) {
            if($(this).attr('href') != '#')
                $(".se-pre-con").fadeIn("slow");
        })

        $(".info-menu").find('a').bind('click', function(e) {
            if($(this).attr('href') != '#')
                $(".se-pre-con").fadeIn("slow");
        })
        
        $(".user-body").find('a').bind('click', function(e) {
            if($(this).attr('href') != '#')
                $(".se-pre-con").fadeIn("slow");
        })
    });

    const ums = {
        success: {
            1000: "문의글을 게시했습니다.",
            2000: "파일을 업로드 했습니다."
        },
        error : {
            1000: "로컬 서버 데이터 요청 에러",
            2000: "파일을 업로드하세요.",
            2001: "엔젤마스터카드를 입력하세요.",
